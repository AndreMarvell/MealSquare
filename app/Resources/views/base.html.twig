<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>
            {% block title %} 
            {% endblock %}
        </title>
            
{#      block css commun à toute les pages  #}

        {% stylesheets 'bundles/mealsquarecommon/css/style.css'
                       'bundles/mealsquarecommon/css/animate.css'
                       'http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'
                       'http://fonts.googleapis.com/css?family=Raleway:400,300,500,600,700,800'
            filter='cssrewrite' 
        %}
        
        {#<link rel="apple-touch-icon" sizes="57x57" href="/apple-icon-57x57.png">
        <link rel="apple-touch-icon" sizes="60x60" href="/apple-icon-60x60.png">
        <link rel="apple-touch-icon" sizes="72x72" href="/apple-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="76x76" href="/apple-icon-76x76.png">
        <link rel="apple-touch-icon" sizes="114x114" href="/apple-icon-114x114.png">
        <link rel="apple-touch-icon" sizes="120x120" href="/apple-icon-120x120.png">
        <link rel="apple-touch-icon" sizes="144x144" href="/apple-icon-144x144.png">
        <link rel="apple-touch-icon" sizes="152x152" href="/apple-icon-152x152.png">
        <link rel="apple-touch-icon" sizes="180x180" href="/apple-icon-180x180.png">
        <link rel="icon" type="image/png" sizes="192x192"  href="/android-icon-192x192.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="96x96" href="/favicon-96x96.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">#}

        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        
        {% image '@MealSquareCommonBundle/Resources/public/images/favicon/android-icon-192x192.png'  output="images/favicon/favicon192.png" %}        
        <link rel="icon" type="image/x-icon" sizes="192x192" href="{{ asset_url }}" />
        {% endimage %}
        
        {% image '@MealSquareCommonBundle/Resources/public/images/favicon/favicon-32x32.png'  output="images/favicon/favicon32.png" %}        
        <link rel="icon" type="image/x-icon" sizes="32x32" href="{{ asset_url }}" />
        {% endimage %}
        
        {% image '@MealSquareCommonBundle/Resources/public/images/favicon/favicon-96x96.png'  output="images/favicon/favicon96.png" %}        
        <link rel="icon" type="image/x-icon" sizes="96x96" href="{{ asset_url }}" />
        {% endimage %}
        
        {% image '@MealSquareCommonBundle/Resources/public/images/favicon/favicon-16x16.png'  output="images/favicon/favicon16.png" %}        
        <link rel="icon" type="image/x-icon" sizes="16x16" href="{{ asset_url }}" />
        {% endimage %}
    </head>
    <body>
        {% block body %}
        {% endblock %}
        
{#      block javascript commun à toute les pages  #}

        {% javascripts '@MealSquareCommonBundle/Resources/public/js/jquery-1.11.1.min.js'
               '@MealSquareCommonBundle/Resources/public/js/jquery.uniform.min.js'
               '@MealSquareCommonBundle/Resources/public/js/wow.min.js'
               '@MealSquareCommonBundle/Resources/public/js/jquery.slicknav.min.js'
               '@MealSquareCommonBundle/Resources/public/js/scripts.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        
{#      block pour l' Ajout de javascript dans les bocks fille  #}
        
        {% block script %}
        {% endblock %}
    </body>
</html>
